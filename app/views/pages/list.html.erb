<div class="container">
  <div class="row">
    <div class="col-12">
      <% if @list.nil? %>
		<p>Une erreur est survenue</p>
      <% else %>
      	<h1><%= @list["name"] %></h1>

      	<p>Site Web : <%= @list["website"] %></p>
      	<p>Téléphone : <%= @list["phone"] %></p>
      	<p>Mail de contact : <%= @list["email"] %></p>
      <% end %>
    </div>
  </div>
</div>

