html,
body {
  background: white;
}
.full-height {
  min-height: calc(100vh - 86px);
}
.navbar {
  min-height: 65px;
  padding: 0 1rem;
  border-bottom: 1px solid #150cbf;
  margin-bottom: 20px;
  font-weight: 500;
}
.navbar-nav .nav-link {
  margin-right: 30px;
  color: #00a3ff !important;
}
.navbar-nav .active .nav-link {
  color: #150cbf !important;
}
.navbar-brand {
  padding: 0;
  margin-right: 30px;
}
.navbar img {
  height: 65px;
}
